{% extends 'layout.html.twig' %}



{% block stylesheets %}
    {{ parent() }}
   
<link rel="stylesheet" href="{{ asset('public/css/datepicker.css') }}">
 <link rel="stylesheet" href="{{ asset('public/css/bootstrap-select.css') }}"> 
  <link rel="stylesheet" href="{{ asset('public/css/bootstrap-select.css') }}"> 
    <link rel="stylesheet" href="{{ asset('public/css/pikaday-package.css') }}"> 
    <link rel="stylesheet" href="{{ asset('public/css/miestilo.css') }}"> 
{% endblock %}



{% block body %}
    
    {{ parent() }}
	<div class="main container">
            <div class="row well" >
			<div class="col-md-12">
				<div class="page-header">
                                    
					<h2>Expediente</h2>
                   
                      

				</div>
                        </div>
                    
     <div>
      <div class="col-md-4">
             <div class="form-group ">
                 <div class="form-label" style="color: black"><h4>Estado del Proceso</h4></div>
         <span>{{form.tipoexpediente}}<span>
          
			     
			    </div>
                             <div class="form-group ">
                                 <div class="form-label" style="color: #000"><h4>Número de Expediente</h4></div>
                    <span>{{ form.numExpediente }}</span>
              
               
                   
                </div>
                     <div class="form-group  ">
                         
                         <div class="form-label" style="color: #000"><h4>Unidad</h4></div>
			        {{ form.unidad}}
			     
			</div>
                        <div class="form-group">
                            <div class="form-label" style="color: #000"><h4>Delito</h4></div>
			        {{ form.delito }}
			 
			    </div>
                                 
                      
                     
                   
                            </div>                 

               
                
                <div class="col-md-4">
                    <div class="col-md-offset-2">
                     
                             <div class="form-group  ">
          <div class="form-label"style="color: #000"><h4>Fecha de Inicio</h4></div>
                     <span >{{ form.fechainicial|date('d-m-Y') }} </span> 
			    
			  
                      </div>
                             
                    {%if form.fechaentrega %}
