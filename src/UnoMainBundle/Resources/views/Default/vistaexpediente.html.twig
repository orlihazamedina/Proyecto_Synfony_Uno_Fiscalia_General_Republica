          <div class="form-label"style="color: #000"><h4>Fecha de Elevado</h4></div>
                   <span > {{ form.fechaentrega|date('d-m-Y') }} </span> 
                   
                            </div>  {%endif%}
 <div class="form-group">
          <div class="form-label"style="color: #000"><h4>Fecha de Pronóstico</h4></div>
                     <span >{{ form.pronostico|date('d-m-Y')}}</span> 
                   
                </div>
                    
                   {%if form.dEVFISCAL %}
                     <div class="form-group">
          <div class="form-label"style="color: #000"><h4>Fecha Devolución Fiscal</h4> </div>
                     <span >{{ form.dEVFISCAL|date('d-m-Y')}}</span> 
                    
                 
                     </div>  {%endif%}
              
                  {%if form.sIS %}
                  <div class="form-group">
          <div class="form-label"style="color: #000"><h4>SIS</h4></div>
                <span >{{ form.sIS|date('d-m-Y') }}</span>  {%endif%}
           
                
                  
                </div>
                   {%if form.dEVTTP %}
                 <div class="form-group">
          <div class="form-label"style="color: #000"><h4>Fecha Devolución Tribunal Provincial</h4></div>
                    <span > {{ form.dEVTTP|date('d-m-Y')}}</span> 
                   
                   
                </div>  {%endif%}
                
                 
                </div>
               
                </div>
      <div class="col-md-2" >
          
                   <div class="form-group" >
			       <div class="form-label"style="color: #000"><h4>Municipio</h4></div>
			        {{ form.municipio }}
			       
                        </div>
                   {%if form.acusados %}
                   <div class="form-group" >
			       <div class="form-label"style="color: #000"><h4>Acusados PP</h4></div>
			        {{ form.acusados }}
                              </div>{%endif%}    
                                 {%if form.acusadosp %}
                   <div class="form-group" >
			       <div class="form-label"style="color: #000"><h4>Acusados</h4></div>
			        {{ form.acusadosp }}
			       
                        </div>{%endif%}        {%if form.observaciones %}  
                     <div class="form-group">
                <div class="form-label" style="color: #000"><h4>Observaciones</h4></div>
