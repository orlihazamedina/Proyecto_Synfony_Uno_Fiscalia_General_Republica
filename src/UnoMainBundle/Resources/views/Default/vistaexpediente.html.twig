	</div>
  
		
      <div id="rinti">  <a href="{{ path('uno_main_editguardar', { id: form.id }) }}" class="btn btn-sm btn-primary"><li class="glyphicon glyphicon-pencil"></li>
			                            {{ 'Editar'|trans }}
			                        </a>

			                        <a href="{{ path('uno_main_delete', { id: form.id }) }}" class="btn btn-sm btn-danger btn-delete"onclick="return confirm('¿Estás seguro?')"><li class="glyphicon glyphicon-trash "></li>
			                            {{ 'Eliminar'|trans }}
			                        </a></div>
     
	{% endblock %}
       